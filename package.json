{
  "name": "@zhu-hong/qr",
  "version": "1.0.0",
  "description": "support Vue2&3 qrcode component and any javascript environment lib",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.cjs",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./util": {
      "default": "./dist/util.cjs",
      "require": "./dist/util.cjs",
      "import": "./dist/util.mjs",
      "types": "./dist/util.d.ts"
    }
  },
  "keywords": [
    "qr",
    "qrcode",
    "vue qr",
    "vue qrcode"
  ],
  "scripts": {
    "dev": "vite dev",
    "build:site": "vite build",
    "build": "unbuild",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "vue-demi": "latest"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "4.1.0",
    "unbuild": "1.2.1",
    "vite": "4.3.0-beta.7",
    "vue": "3.2.47"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "author": {
    "name": "zhu-hong",
    "email": "zhxhyy@qq.com"
  },
  "sideEffects": false,
  "homepage": "https://github.com/zhu-hong/qr",
  "bugs": {
    "url": "https://github.com/zhu-hong/qr/issues",
    "email": "zhxhyy@qq.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhu-hong/qr.git"
  },
  "license": "MIT"
}
